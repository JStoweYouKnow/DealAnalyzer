{
  "framework": "nextjs",
  "buildCommand": "npm ci --legacy-peer-deps && npm run build:next",
  "devCommand": "npm run dev:next",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "true"
        }
      ]
    }
  ]
}